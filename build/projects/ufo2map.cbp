				<Compiler>
					<Add option="-Wall" />
					<Add option="-pg" />
					<Add option="-g" />
					<Add option="-ffloat-store" />
					<Add option="-D__GNUWIN32__" />
					<Add option="-DWINVER=0x501" />
					<Add option="-DDEBUG" />
				</Compiler>
				<Linker>
					<Add option="-pg -lgmon" />
					<Add library="winmm" />
				</Linker>
			</Target>
			<Target title="linux">
				<Option platforms="Unix;" />
				<Option output="../../ufo2map" prefix_auto="0" extension_auto="0" />
				<Option object_output=".objs/ufo2map" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="`sdl-config --cflags`" />
				</Compiler>
				<Linker>
					<Add option="`sdl-config --libs`" />
				</Linker>
			</Target>
			<Target title="macosx">
				<Option platforms="Mac;" />
				<Option output="../../ufo2map" prefix_auto="0" extension_auto="0" />
				<Option object_output=".objs/ufo2map" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add directory="/sw/include" />
					<Add directory="/Library/Frameworks/SDL.framework/Versions/A/Headers" />
				</Compiler>
				<Linker>
					<Add option="-framework SDL" />
					<Add directory="/sw/lib" />
				</Linker>
			</Target>
		</Build>
