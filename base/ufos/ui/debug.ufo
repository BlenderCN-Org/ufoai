// ==================
// EDITOR MENU
// ==================

window editor {
	{
		pos "0 0"
		size "1 1"
	}

	editor editor {
		pos "0 0"
		size "1 1"
	}

	func init {
		cmd "echo \"Use mn_extract to write your changes to a file\";"
		cmd "mn_editornode_start <path:root>.editor"
	}

	func close {
		cmd "mn_editornode_stop <path:root>.editor"
	}
}
