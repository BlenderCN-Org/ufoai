// Concept of using dummy techs for extended requirements (they should not be displayed anywhere - thus the "logic" type)
// These entries can later be moved in front of the rs_craft_weapon_particle_beam entry.

tech rs_craft_ufo_fighter_AND_craft_weapon_particle_beam
{
	type	logic
	name	"LOGIC:Fighter AND 1beamweapon"
	require_AND
	{
		item craft_weapon_particle_beam 1
		tech rs_craft_ufo_fighter
	}
}

tech rs_craft_ufo_harvester_AND_craft_weapon_particle_beam
{
	type	logic
	name	"LOGIC:Harvester AND 1beamweapon"
	require_AND
	{
		item craft_weapon_particle_beam 1
		tech rs_craft_ufo_harvester
	}
}
